//Question: https://leetcode.com/problems/climbing-stairs/description/

//Code:
class Solution {
public:
    int climbStairs(int n) {
        if(n==1)    return 1;
        int a=1,b=1,c=0;
        for(int i=0;i<n-1;i++){
            c=a+b;
            b=a;
            a=c;
        }
        return c;
    }
};

//TC: O(N)

//Approach:  DP
